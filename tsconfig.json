{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "module": "ES6",
    "moduleResolution": "Node16",
    "noImplicitAny": false,
    "paths": {
      "assets/*": ["assets/*"],
      "capableApi/*": ["capableApi/*"],
      "components/*": ["components/*"],
      "config/*": ["config/*"],
      "constants/*": ["constants/*"],
      "dataModels/*": ["dataModels/*"],
      "features/*": ["features/*"],
      "fetchDataHooks/*": ["fetchDataHooks/*"],
      "hooks/*": ["hooks/*"],
      "layouts/*": ["layouts/*"],
      "models/*": ["models/*"],
      "pages/*": ["pages/*"],
      "styles/*": ["styles/*"],
      "utils/*": ["utils/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6"
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.js"]
}
